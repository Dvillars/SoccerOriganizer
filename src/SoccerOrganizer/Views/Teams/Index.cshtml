@{
    ViewData["Title"] = "Soccer List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<SoccerOrganizer.Models.Team>

<h2>Teams</h2>
<h4>@Html.DisplayNameFor(model => model.Name)</h4>
<ul>
    @foreach(var team in Model)
    {
        <li>@Html.DisplayFor(modelTeam => team.Name)</li>

    }
</ul>
